{
  "name": "linkfree",
  "version": "1.10.0",
  "private": false,
  "homepage": "https://linkfree.eddiehub.org",
  "dependencies": {
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "js-abbreviation-number": "^1.4.0",
    "mongoose": "^6.6.5",
    "next": "^13",
    "prettier": "^2.7.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-markdown": "^8.0.3",
    "tailwindcss": "^3.1.8"
  },
  "scripts": {
    "dev": "next dev",
    "dev-wsl": "HOST=$(hostname -I | awk '{print $1}') && next dev --hostname ${HOST}",
    "build": "next build",
    "test": "npx playwright test",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "ncp ./.env.example ./.env",
    "create": "npx linkfree-cli"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@playwright/test": "^1.27.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "eslint": "8.19.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.2",
    "linkfree-cli": "^1.4.0",
    "lint-staged": "^12.1.4",
    "ncp": "^2.0.0",
    "postcss": "^8.4.17"
  },
  "description": "LinkFree connect audiences to all of your content with just one link. It is an open-source alternative to [Linktree](https://linktr.ee/) implemented in JavaScript - It was initially created on a YouTube [live stream](https://www.youtube.com/watch?v=Jorl_vcp-Ew).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EddieHubCommunity/LinkFree.git"
  },
  "keywords": [
    "javascript",
    "linktree",
    "socials",
    "eddiehub",
    "reactjs",
    "react"
  ],
